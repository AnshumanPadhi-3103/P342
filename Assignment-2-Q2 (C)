#include<stdio.h>


int main(){
int A[3]={1,2,3};
int B[3]={4,5,6};

int Sum[3];
int i;
int Dot=0;


for(i=0;i<3;i++){
Sum[i]=A[i]+B[i];  
Dot+=A[i]*B[i];   
}

printf("\nSum of vectors A+B: ");
    for(i=0;i<3;i++)
 
    {
        printf("%d  ", Sum[i]);
    }
 
printf("Dot Product of vectors A.B: %d ",Dot);

return 0;
}


/*
Sum of vectors A+B: 5  7  9
Dot Product of vectors A.B: 32
*/
