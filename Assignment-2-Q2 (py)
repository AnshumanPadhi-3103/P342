a = [1, 2, 3]
b = [4, 5, 6]
n = 3
dotproduct = 0
# Loop for calculate dot product
for i in range(n):
   dotproduct = dotproduct + a[i] * b[i]
print("Dot Product of vectors A.B is: ",dotproduct)

add = []
add.append(a[1]+b[1])
add.append(a[2]+b[2])
add.append(a[2]+b[2])
print("Sum of vectors A+B is:  ",add)
