

#include <stdio.h>

int main()
{
    int M[3][3];
    int N[3][3];
    
    FILE *tmp1=fopen("M.txt","r");
    if(tmp1==NULL){
        printf("ERRor:404, file not found!");
    }
    else{
        fscanf(tmp1,"%d,%d,%d \n,%d,%d,%d \n,%d,%d,%d \n",&M[0][0],&M[0][1],&M[0][2],&M[1][0],&M[1][1],&M[1][2],&M[2][0],&M[2][1],&M[2][2]);
    }
    
    FILE *tmp2=fopen("N.txt","r");
    if(tmp2==NULL){
        printf("ERRor:404, file not found!");
    }
    
    else{
        fscanf(tmp2,"%d,%d,%d \n,%d,%d,%d \n,%d,%d,%d \n",&N[0][0],&N[0][1],&N[0][2],&N[1][0],&N[1][1],&N[1][2],&N[2][0],&N[2][1],&N[2][2]);
    }
    
    int A[]={1,2,3};
    int MA[3]={0,0,0};
    int MN[3][3];
    
    
    
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            MA[i]+=M[i][j]*A[j];
        }
        
    }
    
    for(int i=0;i<3;i++){
        for(int j=0;j<3;j++){
            for(int k=0;k<3;k++){
                MN[i][j]+=M[i][k]*N[k][j];
            }
            
        }
    }
    
    
   printf("MA: \n");
   for(int i=0; i<3; i++) {
      printf("%d ", MA[i]);
         if(i==2){
            printf("\n");
         }
   }
   
   printf("MN: \n");
   for(int i=0; i<3; i++) {
      for(int j=0;j<3;j++) {
         printf("%d ", MN[i][j]);
         if(j==2){
            printf("\n");
         }
      }
   }
   
   

    return 0;
}
