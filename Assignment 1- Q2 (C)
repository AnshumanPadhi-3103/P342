#include <stdio.h>
int main()
{
    int n;
    printf("Enter n to find out n!");
    scanf("%d", &n);
    
    while(n<=0)
    {
        printf("Invalid Input! Enter a positive integer-");
        scanf("%d", &n);
    }
    
    int frac=1;
    
    for (int i=1;i<=n;i++){
        frac*=i;
    }
    printf("Factorial: %d \n",frac);
}   
